{
    "manifestVersion": 4,
    "name": "易修",
    "version": "1.0.0",
    "host": {
        "app": "PS",
        "minVersion": "23.0.0"
    },
    "main": "index.html",
    "entrypoints": [
        {
            "id": "com.listen2me.jwautofill",
            "label": {
                "default": "选区笔"
            },
            "type": "panel",
            "minimumSize": {
                "width": 250,
                "height": 410 
            },
            "maximumSize": {
                "width": 250,
                "height": 868
            },
            "preferredDockedSize": {
                "width": 250,
                "height": 868

            },
            "preferredFloatingSize": {
                "width": 250,
                "height": 868
            },
            "icons": [
                {
                    "path": "icons/light-panel.png",
                    "width": 23,
                    "height": 23,
                    "scale": [1,2],
                    "theme": ["darkest", "dark", "medium"],
                    "species": ["chrome"]
                },
                {
                    "path": "icons/dark-panel.png",
                    "width": 23,
                    "height": 23,
                    "scale": [1,2],
                    "theme": ["lightest", "light"],
                    "species": ["chrome"]
                } 
            ]
        },
        {
            "id": "com.listen2me.pixeladjustment",
            "label": {
                "default": "特制绘画滤镜"
            },
            "type": "panel",
            "minimumSize": {
                "width": 300,
                "height": 410 
            },
            "maximumSize": {
                "width": 300,
                "height": 868
            },
            "preferredDockedSize": {
                "width": 300,
                "height": 868
            },
            "preferredFloatingSize": {
                "width": 300,
                "height": 868
            },
            "icons": [
                {
                    "path": "icons/light-panel.png",
                    "width": 23,
                    "height": 23,
                    "scale": [1,2],
                    "theme": ["darkest", "dark", "medium"],
                    "species": ["chrome"]
                },
                {
                    "path": "icons/dark-panel.png",
                    "width": 23,
                    "height": 23,
                    "scale": [1,2],
                    "theme": ["lightest", "light"],
                    "species": ["chrome"]
                } 
            ]
        }
    ],
    "id": "com.listen2me.jwautofill",
    "icons": [
        {
            "path": "icons/plugin.png",
            "width": 48,
            "height": 48,
            "scale": [1,2],
            "theme": ["darkest", "dark", "medium", "lightest", "light", "all"],
            "species": ["pluginList"]
        }
    ],
    "requiredPermissions": {
        "clipboard": "readAndWrite",
        "network": {
            "domains": "all"
        },
        "allowCodeGenerationFromStrings": true,
        "launchProcess": {
        "schemes": ["http", "https"]
        },
        "localFileSystem": "fullAccess"
    },
    "build": {
        "cssResources": ["styles.css"],
        "featureFlags": {
            "CSSNextSupport": true
        }
    }
}
